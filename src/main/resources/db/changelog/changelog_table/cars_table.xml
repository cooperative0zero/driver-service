<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="car_table_setup" author="Aleksej">
        <createTable tableName="cars">
            <column name="c_id" type="bigint" autoIncrement="true" />
            <column name="c_color" type="varchar(50)" />
            <column name="c_model" type="varchar(255)" />
            <column name="c_vehicle_number" type="varchar(20)" />
            <column name="c_deleted" type="boolean" />
        </createTable>
    </changeSet>

</databaseChangeLog>
